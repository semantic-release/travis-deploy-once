{
  "name": "travis-deploy-once",
  "description": "Run a deployment script only once in the Travis test matrix",
  "version": "0.0.0-development",
  "author": "Stephan BÃ¶nnemann <stephan@boennemann.me>",
  "ava": {
    "require": [
      "./babel-register"
    ]
  },
  "bin": {
    "travis-deploy-once": "bin/travis-deploy-once.js"
  },
  "bugs": {
    "url": "https://github.com/semantic-release/travis-deploy-once/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chalk": "^2.1.0",
    "execa": "^1.0.0",
    "got": "^9.1.0",
    "p-retry": "^2.0.0",
    "semver": "^5.4.1",
    "update-notifier": "^2.3.0",
    "url-join": "^4.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "clear-module": "^3.0.0",
    "codecov": "^3.0.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "nock": "^10.0.0",
    "nyc": "^12.0.1",
    "proxyquire": "^2.0.0",
    "semantic-release": "^15.0.0",
    "sinon": "^6.0.0",
    "xo": "^0.23.0"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "bin",
    "lib",
    "babel-register.js",
    "cli.js",
    "index.js"
  ],
  "homepage": "https://github.com/semantic-release/travis-deploy-once#readme",
  "keywords": [
    "after",
    "deploy",
    "success",
    "travis"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "babel-register.js",
      "cli.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-release/travis-deploy-once.git"
  },
  "scripts": {
    "cm": "git-cz",
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "xo",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
